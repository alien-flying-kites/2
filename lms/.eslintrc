{
  "extends": "eslint-config-egg",
  "rules": {
    "skipBlankLines": true,
    "ignoreComments": true,
    "no-unused-vars": "off",
    "comma-dangle": ["error", {
      "arrays": "never",
      "objects": "ignore",
      "imports": "never",
      "exports": "never",
      "functions": "ignore"
    }],
    "object-shorthand": "off",
    "eol-last": "off",
    "object-curly-spacing": "off",
    "indent": "off",
    "spaced-comment": "off",
    "quotes": "off",
    "prefer-const": "off",
    "space-before-blocks": "warn",
    "space-before-function-paren": "warn",
    "one-var-declaration-per-line": "off",
    "no-var": "off",
    "no-bitwise": "off",
    "valid-jsdoc": "off",
    "linebreak-style": [0 ,"error", "windows"]
  }
}